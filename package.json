{
  "name": "ng-selenium-tools",
  "version": "1.0.6",
  "description": "Angular Selector",
  "main": "dist/src/index.js",
  "scripts": {
    "transpile": "ngc",
    "package": "rollup -c",
    "minify": "uglifyjs dist/src/bundles/ng-selector.module.umd.js --screw-ie8 --compress --mangle --comments --output dist/src/bundles/ng-selector.module.umd.min.js",
    "build": "npm run transpile && npm run package && npm run minify"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dennisfleischmann/ng-selenium-selector.git"
  },
  "author": "Dennis Fleischmann",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dennisfleischmann/ng-selenium-selector/issues"
  },
  "homepage": "https://github.com/dennisfleischmann/ng-selenium-selector#readme",
  "devDependencies": {
    "@angular/compiler": "^5.0.5",
    "@angular/compiler-cli": "^5.0.5",
    "rollup": "^0.52.0",
    "typescript": "2.4.2",
    "uglify-js": "^3.2.0"
  },
  "dependencies": {
    "@angular/core": "^5.0.5"
  },
  "files": [
    "dist"
  ]
}
